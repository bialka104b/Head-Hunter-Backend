# Editor configuration, see http://editorconfig.org
# INstall EditorConfig for VS Code v0.16.4
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = crlf

[*.ts]
indent_style = tab
indent_size = 4

[*.html]
indent_style = tab
indent_size = 4

[*.json]
indent_style = tab
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = true
